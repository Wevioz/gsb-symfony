<form method="post" action="{{ path('praticien_delete', {'praNum': praticien.praNum}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ praticien.praNum) }}">
    <button class="btn">Delete</button>
</form>
