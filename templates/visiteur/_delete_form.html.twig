<form method="post" action="{{ path('visiteur_delete', {'visMatricule': visiteur.visMatricule}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ visiteur.visMatricule) }}">
    <button class="btn">Delete</button>
</form>
